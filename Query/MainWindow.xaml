                    <DataGrid AutoGenerateColumns="False" Height="293" HorizontalAlignment="Left" Name="amDataGridService" VerticalAlignment="Top" Width="457" ColumnHeaderHeight="{StaticResource {x:Static SystemParameters.CursorHeightKey}}" ItemsSource="{Binding}" MinColumnWidth="87" VerticalGridLinesBrush="#FF908E8E" HorizontalGridLinesBrush="#FF555555" Margin="-2,0,0,0" SelectionMode="Single" SelectionChanged="amDataGridService_SelectionChanged" CanUserReorderColumns="False">
