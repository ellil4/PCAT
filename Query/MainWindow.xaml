                            <DataGridTextColumn Header="组别" Binding="{Binding GroupMark}" />
                            <DataGridTextColumn Header="状态" Binding="{Binding Status}" />
                            <DataGridTextColumn Header="IP" Binding="{Binding IP}" />
                            <DataGridTextColumn Header="机器名" Binding="{Binding MachineName}" />
                            <DataGridTextColumn Header="被试名称" Binding="{Binding SubjectName}" />
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
