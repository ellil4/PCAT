<Window x:Class="Query.PasswordRequestPan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="密码输入" Height="92" Width="355" ResizeMode="NoResize" Closing="Window_Closing">
    <Grid>
        <Label Content="管理密码：" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="78,13,0,0" Name="amPasswordBox" VerticalAlignment="Top" Width="163" KeyDown="amPasswordBox_KeyDown" />
        <Button Content="确定" Height="23" HorizontalAlignment="Left" Margin="250,13,0,0" Name="amBtnConfirm" VerticalAlignment="Top" Width="75" Click="amBtnConfirm_Click" />
    </Grid>
</Window>
