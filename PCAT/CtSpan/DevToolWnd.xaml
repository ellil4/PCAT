<Window x:Class="FiveElementsIntTest.CtSpan.DevToolWnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DevToolWnd" Height="102" Width="499" Topmost="True" WindowStyle="ToolWindow" ResizeMode="NoResize">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,28,0,0" Name="amTBTarCount" VerticalAlignment="Top" Width="66" />
        <Label Content="目标数" Height="28" HorizontalAlignment="Left" Margin="12,0,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="84,28,0,0" Name="amTBInterCir" VerticalAlignment="Top" Width="66" />
        <Label Content="干扰圆" Height="28" HorizontalAlignment="Left" Margin="84,0,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="156,28,0,0" Name="amTBInterTri" VerticalAlignment="Top" Width="66" />
        <Label Content="干扰三角" Height="28" HorizontalAlignment="Left" Margin="156,0,0,0" Name="label3" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="228,28,0,0" Name="amTBDisTar" VerticalAlignment="Top" Width="66" />
        <Label Content="目标距离" Height="28" HorizontalAlignment="Left" Margin="228,0,0,0" Name="label4" VerticalAlignment="Top" />
