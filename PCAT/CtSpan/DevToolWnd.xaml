        <Label Content="一般距离" Height="28" HorizontalAlignment="Left" Margin="300,0,0,0" Name="label5" VerticalAlignment="Top" />
        <Button Content="生成" Height="23" HorizontalAlignment="Left" Margin="390,5,0,0" Name="amBtnDo" VerticalAlignment="Top" Width="75" Click="amBtnDo_Click" />
        <Button Content="记录" Height="23" HorizontalAlignment="Left" Margin="390,32,0,0" Name="amBtnSave" VerticalAlignment="Top" Width="75" Click="amBtnSave_Click" />
    </Grid>
</Window>
